<tr>
  <td><%= project_member_stt project_user_counter %></td>
  <td><%= project_user.user_name %></td>
  <td><%= project_user.project_role_name %></td>
  <td><%= project_user.user_git_account %></td>
  <td><%= format_date project_user.joined_at %></td>
  <td><%= format_date project_user.left_at %></td>
  <td>
    <%= link_to "#", class: "show", title: "Show", "data-toggle" => "tooltip" do %>
      <i class="bi bi-eye"></i>
    <% end %>
    <%= link_to "#", class: "edit", title: "Edit", "data-toggle" => "tooltip" do %>
      <i class="bi bi-pencil-square"></i>
    <% end %>
    <%= link_to "#", class: "delete", title: "Delete", "data-toggle" => "tooltip" do %>
      <i class="bi bi-trash"></i>
    <% end %>
  </td>
</tr>
